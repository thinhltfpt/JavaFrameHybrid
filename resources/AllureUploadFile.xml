<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Allure-report upload file jquery" >
    <listeners>
        <listener class-name="commons.MethodListener"/>
        <listener class-name="reportConfig.AllureReportListener"/>
    </listeners>
    <parameter name="url" value="https://blueimp.github.io/jQuery-File-Upload/"/>
    <test name="run Firefox">
        <parameter name="browser" value="firefox"/>
        <classes>
            <class name="com.jquery.upload.Tpic_uploadFile_Allure_report"/>
        </classes>
    </test>

